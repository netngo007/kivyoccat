<ThirdWindow>:
    
    name:"loading"
    canvas.before:
        Color: 
            rgba: (255, 255, 255,1)
        Rectangle:
            pos:self.pos
            size:self.size    
            
    MDFloatLayout:
        do_rotation:False
        Image:
            source: "The stasianery.png"
            allow_stretch:True
